  "bugs": {
    "url": "https://github.com/kunalgolani/keep-warm/issues"
  },
  "homepage": "https://github.com/kunalgolani/keep-warm",
  "devDependencies": {
    "babel": "^5.8.23",
    "eslint": "~1.7.3"
  },
  "dependencies": {
    "co": "~4.6.0",
    "underscore": "^1.8.3"
  }
